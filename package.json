    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-config-standart": "^0.2.0",
    "eslint-plugin-promise": "^4.2.1",
