  "main": "index.js",
  "scripts": {
    "dev": "cross-env-shell NODE_ENV=development run-p webpack:* watch:*",
    "build:server": "webpack --config webpack/server.config.js",
    "watch:client": "webpack --watch --config webpack/client.config.js",
    "watch:server": "cross-env-shell nodemon --inspect build/${npm_package_version}/server.bundle.js",
    "webpack:server": "webpack --watch --config webpack/server.config.js",
    "lint": "eslint **/*.ts",
    "docker:build": "cross-env-shell docker build -t dezmound/land:${npm_package_version} --build-arg VERSION=${npm_package_version} ."
  },
